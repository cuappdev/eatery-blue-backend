  "bugs": {
    "url": "https://github.com/cuappdev/eatery-blue-backend/issues"
  },
  "homepage": "https://github.com/cuappdev/eatery-blue-backend#readme",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "escape-html": "^1.0.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "node-cache": "^5.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/escape-html": "^1.0.4",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^15.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
