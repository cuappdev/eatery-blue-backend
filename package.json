    "start": "node dist/src/server.js",
    "start:prod": "NODE_ENV=production node dist/src/server.js",
    "start:dev": "NODE_ENV=development node dist/src/server.js",
    "dev": "NODE_ENV=development nodemon -x tsx src/server.ts",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "format": "prettier --write '{src,test}/**/*.ts'",
    "scrape": "tsx prisma/scraper.ts",
    "scrape:scheduled": "SCHEDULED_MODE=true tsx prisma/scraper.ts",
    "studio": "prisma studio",
    "send-notifications": "tsx prisma/send-notifications.ts",
    "test-noti-setup": "tsx src/utils/test-notification-setup.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuappdev/eatery-blue-backend.git"
  },
  "keywords": [],
  "author": "",
